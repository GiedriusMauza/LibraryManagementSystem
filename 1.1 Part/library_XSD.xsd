      </xs:element>
      <xs:element name="production-year">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="2023"/>
            <xs:enumeration value="2022"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="publisher">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Eglės leidykla"/>
