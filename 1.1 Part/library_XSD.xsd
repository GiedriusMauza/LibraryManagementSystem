        </xs:simpleType>
      </xs:element>
      <xs:element name="return-date">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="2022-11-25"/>
            <xs:enumeration value="2022-12-16"/>
            <xs:enumeration value="2023-06-26"/>
            <xs:enumeration value="2023-02-09"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element type="itemType" name="item"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="borrowingsType">
    <xs:sequence>
      <xs:element type="borrowingType" name="borrowing" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="subscriberType">
    <xs:sequence>
      <xs:element name="subscriber-id">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
