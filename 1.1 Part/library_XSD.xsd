            <xs:enumeration value="Darius Musteikis"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element type="xs:string" name="status"/>
      <xs:element name="title">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Žvilgsnis iš maišo"/>
            <xs:enumeration value="Naujagimiai"/>
            <xs:enumeration value="Keturi"/>
            <xs:enumeration value="Akis į akį. Klaipėda 1923"/>
            <xs:enumeration value="Dievų miškas"/>
            <xs:enumeration value="Mėguvos Valdovas"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="borrowingType">
    <xs:sequence>
      <xs:element name="borrowing-id">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="borrow-date">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="2022-10-14"/>
            <xs:enumeration value="2021-11-14"/>
            <xs:enumeration value="2021-10-14"/>
            <xs:enumeration value="2021-10-04"/>
            <xs:enumeration value="2023-01-14"/>
